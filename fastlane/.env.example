# App Store Connect API Key Configuration
# Copy this file to .env and fill in your values
# Get these from App Store Connect > Users and Access > Integrations > App Store Connect API

# Key ID (shown in the API keys table, e.g., ABC123XYZ)
APP_STORE_CONNECT_API_KEY_KEY_ID=YOUR_KEY_ID

# Issuer ID (shown at top of API keys page)
APP_STORE_CONNECT_API_KEY_ISSUER_ID=YOUR_ISSUER_ID

# Path to your .p8 key file
APP_STORE_CONNECT_API_KEY_KEY_FILEPATH=~/.appstoreconnect/private_keys/AuthKey_YOUR_KEY_ID.p8
